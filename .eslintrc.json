{
  "extends": ["standard"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "standard",
    "@typescript-eslint",
    "promise"
  ],
  "rules": {
    "no-dupe-class-members": "off",
    "indent": ["error", 4],
    "no-unused-vars": "off",
    "no-use-before-define": "off",
    "no-useless-constructor": "off"
  }
}
